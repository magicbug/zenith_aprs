# AGWPE Configuration
# The host where your AGWPE server is running (usually localhost)
AGWPE_HOST=192.168.1.207
# The port your AGWPE server is listening on (default is 8000)
AGWPE_PORT=8000
# Your amateur radio callsign (required)
CALLSIGN=2M0SQL
# SSID for your callsign (0-15, default is 0)
SSID=0

# WebSocket Configuration
# Host to bind the WebSocket server to (0.0.0.0 for all interfaces)
WS_HOST=0.0.0.0
# Port for the WebSocket server to listen on
WS_PORT=8765

# Debug Mode
# Set to true for detailed debug logging
DEBUG=true